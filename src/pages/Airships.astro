---
import Layout from "../layouts/Layout.astro"
import ModalAddJet from "../components/modalAddJet.astro"
import TableModal from "../components/table/TableModal"

import { getAirships } from "../../lib/actions/airships/actions"
const data = await getAirships()
---

<Layout title="Airships">
	<div class="flex flex-col space-y-4">
		<div>
			<ModalAddJet />
		</div>
		<div>
			<TableModal info={data} caseType="airship" client:load />
		</div>
	</div>
</Layout>
